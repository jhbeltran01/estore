{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "target": "ES5",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "carousel": [
        "ts/components/carousel"
      ],
      "element": [
        "ts/components/elements"
      ],
      "filter": [
        "ts/components/filter"
      ],
      "header": [
        "ts/components/header"
      ],
      "pagination": [
        "ts/components/pagination"
      ],
      "slider": [
        "ts/components/slider"
      ],
      "Types": [
        "ts/types"
      ],
      "hooks": [
        "ts/hooks"
      ],
      "ts": [
        "ts/ts"
      ],
      "json": [
        "ts/json"
      ]
    }
  },
  "include": [
    "src"
  ]
}